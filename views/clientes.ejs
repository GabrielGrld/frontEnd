<!DOCTYPE html>
<html lang="en" dir="ltr">

<head>
  <meta charset="utf-8">
  <title>Clientes</title>

  <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">

</head>

<body>
  <h1>Clientes En el Sistema</h1>
  <table class="table table-striped  table-hover">
    <thead>
    <tr>
      <th scope="col">Id Cliente</th>
      <th scope="col">Identificaci√≥n</th>
      <th scope="col">Nombre</th>
      <th scope="col">Apellido</th>
      <th scope="col">Nacionalidad</th>
      <th scope="col">Telefono</th>
    </tr>
    </thead>

    <tbody>

    <% for(var i=0; i < clientesJSON.length; i++) { %>
      <tr>
        <td scope="row"><%= clientesJSON[i].cliente_id %></td>
        <td><%= clientesJSON[i].identificacion %></td>
        <td><%= clientesJSON[i].nombre %></td>
        <td><%= clientesJSON[i].apellido %></td>
        <td><%= clientesJSON[i].nacionalidad %></td>
        <td><%= clientesJSON[i].telefono %></td>
      </tr>
      <% } %>
      </tbody>
  </table>

  <form class="" action="/seeusers" method="get">
    <button type="submit" name="button">Mirar Clientes</button>

  </form>





    </body>
</body>

</html>
